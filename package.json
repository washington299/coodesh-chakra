    "@typescript-eslint/parser": "^4.28.0",
    "babel-loader": "^8.2.5",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.5.11",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "jest-dom": "^4.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.2",
    "typescript": "^4.3.4"
  }
